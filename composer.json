{
    "name": "meq/okta-api",
    "autoload": {
        "psr-4": {
            "Meq\\OktaApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meq\\Tests\\OktaApi\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "meQuilibrium"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/var-dumper": "^4.4"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "sniff": "vendor/bin/phpcs",
        "sniff-fix": "vendor/bin/phpcbf",
        "stan": "vendor/bin/phpstan"
    }
}
